{
    "name": "watson/active",
    "description": "Laravel 4 helper for recognising the current route, controller and action",
    "keywords": ["laravel", "active", "routing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/http": "~4.2 || ~5.0",
        "illuminate/routing": "~4.2 || ~5.0",
        "illuminate/support": "~4.2 || ~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Watson\\Active\\": "src/"
        },
        "files": [
            "src/ActiveHelpers.php"
        ]
    },
    "minimum-stability": "stable"
}
